# Base URL for the Ollama API (inside Docker we map host-gateway)
OLLAMA_BASE_URL=http://host.docker.internal:11434

# Qdrant configuration
QDRANT_URL=http://qdrant:6333
QDRANT_API_KEY=
QDRANT_COLLECTION=lcod_docs

# Retrieval defaults
EMBEDDING_MODEL=nomic-embed-text
GENERATIVE_MODEL=llama3.1
TOP_K=5

# API server
API_HOST=0.0.0.0
API_PORT=8088
